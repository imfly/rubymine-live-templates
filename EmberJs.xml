      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="case" value="switch($VAR$)&#10;{&#10;case $Value1$:&#10;  $Expression$&#10;  break;&#10;case $Value2$:&#10;  $Expression$&#10;  break;&#10;default:&#10;  $End$&#10;}" description="Inserts case expression" toReformat="false" toShortenFQNames="true">
